{
  "name": "vuejs-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "dotenv -e .env -- supabase link --project-ref \"${SUPABASE_PROJECT_REF}\"",
    "supabase:types": "supabase gen types typescript --project-id=rdpeftfiezcmhdtqfaav --schema public > database/types/supabase.ts",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.8",
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "iconify-icon": "^2.2.0",
    "lucide-vue-next": "^0.468.0",
    "node-fetch": "^3.3.2",
    "pinia": "^2.3.0",
    "radix-vue": "^1.9.11",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "supabase": "^2.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^6.0.3",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
